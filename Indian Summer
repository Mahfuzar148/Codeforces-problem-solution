#include<bits/stdc++.h>
using namespace std;
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);
    int n;
    cin>>n;
    set<string>st;
    while(n--)
    {
        string s;
        char c;
        cin>>c;
        getline(cin,s);
        s = s + c;
        st.insert(s);
    }
      cout<<st.size()<<endl;
}
